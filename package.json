{
  "name": "webpack-demo",
  "version": "1.0.0",
  "description": "First webpack demo",
  "repository": "test",
  "browserslist": "> 0.25%, not dead",
  "private": false,
  "scripts": {
    "build": "cross-env NODE_ENV=dev webpack --mode development",
    "watch": "cross-env NODE_ENV=dev webpack --mode development --watch",
    "build:prod": "cross-env NODE_ENV=prod webpack --mode production",
    "start": "cross-env NODE_ENV=dev webpack-dev-server --mode development --open",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "keywords": [
    "webpack",
    "demo"
  ],
  "author": "Dmitrii Kupchenko <kupchenko.work@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "^2.3.4",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3"
  }
}
